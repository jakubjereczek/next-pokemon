{
  "name": "triple-boilerplate-nextjs",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "engines": {
    "npm": ">=10",
    "node": ">=20"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "scripts": {
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "start": "node server.js",
    "build": "next build",
    "dev": "next dev",
    "test:unit": "vitest",
    "test:coverage": "vitest --run --coverage",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "lint": "concurrently -c \"auto\" --kill-others-on-fail \"npm run lint:*(!fix)\"",
    "lint:css": "stylelint \"src/**/*.scss\" --color",
    "lint:eslint": "eslint src --color",
    "lint:tsc": "tsc --noEmit --pretty",
    "lint:fix": "concurrently -c \"auto\" \"npm run lint:*(!fix) -- --fix\"",
    "fix": "concurrently -c \"auto\" \"npm run *:fix\" \"npm run prettier:fix\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@radix-ui/react-checkbox": "^1.1.3",
    "@radix-ui/react-dialog": "^1.1.5",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-radio-group": "^1.2.2",
    "@radix-ui/react-slot": "^1.1.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "dotenv": "^16.4.5",
    "lucide-react": "^0.462.0",
    "next": "^14.2.29",
    "next-intl": "^3.25.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup": "^4.28.0",
    "tailwind-merge": "^2.6.0",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.16.0",
    "@next/env": "^14.1.0",
    "@next/eslint-plugin-next": "^15.0.3",
    "@svgr/webpack": "^8.1.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.3",
    "@vitest/eslint-plugin": "^1.1.25",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.1.2",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-triple": "^2.0.4",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest-dom": "^5.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-no-secrets": "^1.1.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-testing-library": "^7.1.1",
    "globals": "^15.13.0",
    "husky": "^9.1.7",
    "jsdom": "^25.0.1",
    "lint-staged": "^16.0.0",
    "postcss": "^8.5.1",
    "postcss-import": "^16.1.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "stylelint": "^16.13.2",
    "stylelint-config-standard": "^37.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.3"
  }
}
